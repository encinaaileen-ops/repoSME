<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta http-equiv="cache-control" content="private" />
    <meta http-equiv="pragma" content="no-cache" />
    <meta name="referrer" content="no-referrer" />
    <link rel="apple-touch-icon" sizes="180x180" href="/Images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/Images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/Images/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#00aba9">
    <meta name="msapplication-TileColor" content="#00aba9">
    <meta name="theme-color" content="#ffffff">

    <title></title>
    @RenderSection("Styles", required: false)
    <!--STYLES-->
    <link rel="stylesheet" type="text/css" href="/Content/preloader.css">
    @Styles.Render("~/font-awesome/css")
    @Styles.Render("~/Content/kendo/css")
    @Styles.Render("~/Content/css")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/plugins/validate")
    @Scripts.Render("~/bundles/jqueryunobtrajax")
    @Scripts.Render("~/bundles/bnetflex")
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/knockout")
    <script src="~/Scripts/custom/shared/fixvalidation.js"></script>
    <script type="text/javascript" src="~/Scripts/custom/common/kendoo.datasource.extension.js"></script>
    <script src="~/Scripts/expressive.annotations.validate.js"></script>

    @Html.Partial("_LayoutHeaderScripts")

    <style type="text/css">
        .special-link, .special-link:visited, .special-link:hover, .special-link:active {
            color: inherit;
            font-weight: 100;
            background-color: inherit;
        }

        div.modal div.modal-backdrop {
            z-index: 1 !important;
        }

        textarea {
            resize: none;
        }

        td[role=gridcell] .btn {
            margin-bottom: 0;
        }

        #treeViewSearchInput0::-ms-clear {
            display: none;
        }
    </style>

</head>
<body class="fuelux" style="overflow-y: auto;">
    <div id="wrapper">
        <!--Page Preloader - COMMENTED OUT FOR NOW -->
        @*<div id="preloader_anim">
            <div class="circle"></div>
            <div class="circle1"></div>
        </div>*@
        <!-- Navigation -->
        @Html.Partial("_Menu")

        <div id="page-wrapper" class="gray-bg">

            @Html.Partial("_TopNavbar")

            <div id="titleAndBCContainer" class="row wrapper border-bottom white-bg page-heading" style="padding-bottom: 10px;">
                <div class="col-lg-12">
                    <div style="padding-top: 10px; margin-top: 0;">
                        <h2 style="width: 80%; display: inline">@ViewBag.Header</h2>
                        <a style="margin-top: 10px;" id="showGlobalSearchBtn" class="pull-right special-link padding-lg-top">Search <i class="fa fa-search "></i></a>
                    </div>
                    @* Breadcrumb placeholder *@
                    <ol class="breadcrumb">
                        <li><a href="#">Home</a></li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>

            <div class="wrapper wrapper-content animated fadeInRight" id="systemMessageBox" style="display: none">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="alert alert-info" style="margin-bottom:1px;">
                        </div>
                    </div>
                </div>
            </div>

            <div class="wrapper wrapper-content">
                @*<div id="preloader_bg"></div>*@
                <div id="pageBody">
                    @RenderBody()
                </div>
            </div>
            @Html.Partial("_Footer")
        </div>
        <!-- End page wrapper-->
    </div>

    <!-- End wrapper-->
    <!-- Section for main scripts render -->

    @Scripts.Render("~/bundles/bootstrap")

    @Scripts.Render("~/plugins/slimScroll")


    @*<section inspinia bundle>*@

    @*this bundles separated for fix slimScroll in menu*@

    @*@Scripts.Render("~/bundles/inspinia")*@

    @Scripts.Render("~/bundles/inspiniaMetisMenu")
    @Scripts.Render("~/bundles/inspiniaPace")
    <script src="/Scripts/app/inspinia.js"></script>

    @*</section>*@



    @Scripts.Render("~/bundles/other")
    <!-- Skin config script - only for demo purpose-->
    @Scripts.Render("~/bundles/skinConfig")


    <!-- Handler for local scripts -->
    @RenderSection("scripts", required: false)

    @Html.Partial("_LayoutScripts")
</body>
</html>
